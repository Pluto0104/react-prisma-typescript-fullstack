
type User {
  id: ID! @id
  email: String @unique
  verifiedEmail: Boolean! @default(value: false)
  name: String!
  posts: [Post!]!
  password: String!
  test: String!
}

type Post {
  id: ID! @id
  title: String!
  published: Boolean! @default(value: false)
  author: User @relation(link: INLINE)
}

type Test {
  id: ID! @id
  lol: String!
}